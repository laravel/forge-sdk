{
    "name": "laravel/forge-sdk",
    "description": "The official Laravel Forge PHP SDK.",
    "keywords": ["laravel", "forge"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/forge-sdk/issues",
        "source": "https://github.com/laravel/forge-sdk"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Mohamed Said",
            "email": "mohamed@laravel.com"
        },
        {
            "name": "Dries Vints",
            "email": "dries@laravel.com"
        },
        {
            "name": "James Brooks",
            "email": "james@laravel.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3.1|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Forge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
